{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"cli": {
		"packageManager": "npm"
	},
	"newProjectRoot": "projects",
	"projects": {
		"root": {
			"projectType": "application",
			"schematics": {},
			"root": "projects/root",
			"sourceRoot": "projects/root/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/root",
						"index": "projects/root/src/index.html",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/root/tsconfig.app.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/root/public",
								"output": "."
							}
						],
						"styles": [
							"projects/root/src/styles.css"
						],
						"scripts": [],
						"main": "projects/root/src/main.ts",
						"extraWebpackConfig": "projects/root/webpack.config.js",
						"commonChunk": false,
						"vendorChunk": true,
						"namedChunks": true
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/root/webpack.prod.config.js"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "root:build:production",
							"extraWebpackConfig": "projects/root/webpack.prod.config.js"
						},
						"development": {
							"buildTarget": "root:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4200,
						"publicHost": "http://localhost:4200",
						"extraWebpackConfig": "projects/root/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/root/tsconfig.spec.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/root/public"
							}
						],
						"styles": [
							"projects/root/src/styles.css"
						],
						"scripts": []
					}
				}
			}
		},
		"remote": {
			"projectType": "application",
			"schematics": {},
			"root": "projects/remote",
			"sourceRoot": "projects/remote/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/remote",
						"index": "projects/remote/src/index.html",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/remote/tsconfig.app.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/remote/public",
								"output": "."
							}
						],
						"styles": [
							"projects/remote/src/styles.css"
						],
						"scripts": [],
						"main": "projects/remote/src/main.ts",
						"extraWebpackConfig": "projects/remote/webpack.config.js",
						"commonChunk": false,
						"vendorChunk": true,
						"namedChunks": true
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/remote/webpack.prod.config.js"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "remote:build:production",
							"extraWebpackConfig": "projects/remote/webpack.prod.config.js"
						},
						"development": {
							"buildTarget": "remote:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4201,
						"publicHost": "http://localhost:4201",
						"extraWebpackConfig": "projects/remote/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/remote/tsconfig.spec.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/remote/public"
							}
						],
						"styles": [
							"projects/remote/src/styles.css"
						],
						"scripts": []
					}
				}
			}
		}
	}
}